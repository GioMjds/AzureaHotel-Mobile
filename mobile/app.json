{
  "expo": {
    "name": "Azurea Hotel",
    "slug": "azurea-hotel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "azurea-hotel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.azureahotel",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "cover"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Azurea Hotel to access your photos to save QR codes and payment receipts.",
          "savePhotosPermission": "Allow Azurea Hotel to save GCash QR codes to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "privacyPolicyUrl": "https://giomjds.github.io/AzureaHotel-Mobile/",
      "eas": {
        "projectId": "a84d9bf4-b0da-4a46-b59c-07952582952b"
      }
    }
  }
}
